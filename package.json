{
  "name": "p-town-pos",
  "version": "1.0.0",
  "description": "Point of Sale system for P-Town restaurant",
  "main": "src/App.tsx",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "build:prod": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 3000",
    "deploy:netlify": "npm run build:prod && netlify deploy --prod --dir=build",
    "deploy:firebase": "npm run build:prod && firebase deploy --only hosting",
    "predeploy": "npm run build:prod",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write src/**/*.{ts,tsx,css,json}"
  },
  "dependencies": {
    "express": "^5.1.0",
    "firebase": "^10.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.4",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "netlify-cli": "^23.1.3",
    "prettier": "^3.0.0",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "homepage": ".",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
